#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {r
	
    int n,b,sum;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    scanf("%d",&n);
while(n>=1)
{
 b=n%10;
 sum=sum+b;
 n=n/10;
}
printf("%d",sum);
return 0;
    return 0;
}
