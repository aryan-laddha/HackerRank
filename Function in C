#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int max_of_four(int a,int b,int c,int d);

int main()
{
    int p,q,r,s,w;
    scanf("%d%d%d%d",&p,&q,&r,&s);
    w=max_of_four(p,q,r,s);
    printf("%d",w);
    return 0;
}

int max_of_four(int a,int b,int c,int d)
{
   int largest=0;
    if(a>b && a>c && a>d)
    {
        largest=a;
    }
    else if(b>a && b>c && b>d)
    {
         largest=b;
    }
    else if(c>a && c>b && c>d)
    {
        largest=c;
    }
    else
    {
         largest=d;

    }
    return (largest);

}
